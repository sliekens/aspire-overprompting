{
    "name": "Overprompting",
    "image": "mcr.microsoft.com/devcontainers/base:bookworm",
    "features": {
        "ghcr.io/devcontainers/features/docker-in-docker:2": "latest",
        "ghcr.io/devcontainers/features/dotnet": {
            "version": "10.0.100",
            "aspNetCoreRuntimeVersions": "9.0"
        },
        "./features/aspire-cli": {
            "quality": "dev"
        }
    },
    "onCreateCommand": {
        "workloads": "sudo dotnet workload update",
        "dev-cert": "bash .devcontainer/setup-dotnet-dev-cert.sh "
    }
}